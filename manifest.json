{
    "manifest_version": 3,
    "name": "Coronavirus Immunity",
    "version": "0.1.0",
    "description": "A Chrome extension that alters coronavirus mentions on webpages.",
    "host_permissions": [
      "<all_urls>"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["script.js"],
        "css": ["styles.css"]
      }
    ]
  }
  